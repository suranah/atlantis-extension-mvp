{
    "name": "Tabula Rasa 9",
    "description": "Tabula Rasa is a new extension!",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["activeTab", "tabs"],
    "content_scripts": [
      {
        "matches": ["https://www.youtube.com/*"],
        "js": ["content.js"]
      }
    ],  
    "action": {
      "default_popup": "tabula.html",
      "default_icon": "hello_extensions.png"
    }
  }